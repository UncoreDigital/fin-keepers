import List from "../components/casestudy/List4.astro";
import ContactResp from "../components/home/animated/ContactResp.astro";
import Footer6 from "../components/Footer6.astro";
---

<Layout 
  title="Client Success Stories | Bookkeeping Case Studies | Finkeepers"
  description="Discover how Finkeepers helped businesses improve their financial management. Real case studies showcasing our bookkeeping and financial services success."
  canonical="https://finkeepers.us/casestudy"
>
  <Header5 />
  <div class="mt-20">
    <Hero />
  </div>
  <div class="mt-10 mb-10">
    <List />
  </div>
  <div class="mt-10">
    <ContactResp />
    <div class="relative z-0 -mt-[100px]">
      <Footer6 />
    </div>
  </div>
